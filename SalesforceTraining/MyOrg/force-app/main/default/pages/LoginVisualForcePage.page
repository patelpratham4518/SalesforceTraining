<apex:page standardController="Student__c" showHeader="false">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection title="Regestartion Page" columns="2">
                <apex:inputField value="{!Student__c.Name}" required="true"/>
                <apex:inputField value="{!Student__c.Roll_No__c}" required="true"/>
           		<apex:inputField value="{!Student__c.Coursee__c}"/>
                <apex:inputField value="{!Student__c.Password__c}" required="true"/>
                <apex:inputField value="{!Student__c.Club__c}"/>
            </apex:pageBlockSection>
            
    <apex:pageBlockButtons >
          <apex:commandButton action="{!Save}" value="Register"/>
    </apex:pageBlockButtons>
        </apex:pageBlock>
    
    </apex:form>
</apex:page>